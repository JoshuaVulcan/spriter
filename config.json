{
    "mode": {
        "symbol": {
            "sprite": "../event-svg-sprite.svg",
            "render": {
                "css": false
            }
        }
    },
    "dest": "../dist/public/assets/images/",
    "shape": {
        "transform": [
            { "svgo": {
                "plugins": [
                    { "removeStyleElement": true },
                    { "removeAttrs": { "attrs": ["class", "style", "fill", "stroke"] } }
                ]
            }}
        ]
    }
}